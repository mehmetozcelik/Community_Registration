<table class="table" style="width: 70%; margin: auto; border: solid; border: solid; border-color:lightgrey;border-width: 2px">
    <thead>
        <tr>
            <th>Sıra No</th>
            <th>Görev Adı</th>
            <th>Adı Soyadı</th>
            <th>Sil</th>
        </tr>
    </thead>
    <tbody class="kisi">
        @{
            int i = 0;
            foreach (var item in ViewBag.yk1)
            {
                i++;
                <tr>
                    <td>@i</td>
                    <td>@item.unvan</td>
                    <td>@item.adi @item.soyadi</td>
                    <td class="sil"><a href="/Guncelle/uyeListesiSil/@item.ID" class="btn btn-danger btn-xs itemDelete"> <i class="icon-trash"> Sil</i></a></td>
                </tr>
            }
        }
    </tbody>
</table>
